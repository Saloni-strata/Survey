<h1>User <%= @user.uemail %> Response for <%= @user.surveydetail.surveyname%> Survey</h1>

<div>


<table>
  <tr>
    <th>Question</th>
    <th>Answer</th>
  </tr>
  <% @user.user_responses.each do |user_response| %>
  <tr>
    <td><%= user_response.question.que%></td>
    <td><%= user_response.answer%></td>
  </tr>
<% end %>
</table>

<br />
</div>